@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@addTagHelper *, Progress.Sitefinity.AspNetCore

@using Progress.Sitefinity.AspNetCore.ViewComponents
@using WebApp.ViewModels.StaticSection

@model StaticSectionViewModel
<section style="border-radius: 20px; background-color: red;" class="row">
    <div class="col" data-sfcontainer container-context="@this.Model.Context.ContainerContext("Column1", "Column 1")">
    </div>
    <div class="col" data-sfcontainer container-context="@this.Model.Context.ContainerContext("Column2", "Column 2")">
    </div>
    <div class="col" data-sfcontainer container-context="@this.Model.Context.ContainerContext("Column3", "Column 3")">
    </div>
    <div class="col" data-sfcontainer container-context="@this.Model.Context.ContainerContext("Column4", "Column 4")">
    </div>
</section>
