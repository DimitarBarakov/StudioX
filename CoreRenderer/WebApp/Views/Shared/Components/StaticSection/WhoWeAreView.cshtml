@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@addTagHelper *, Progress.Sitefinity.AspNetCore


@using Progress.Sitefinity.AspNetCore.ViewComponents
@using WebApp.ViewModels.StaticSection

@model StaticSectionViewModel


<section class="photo-left-links mb-115-86" >
    <div class="wrapper wrapper--inner">
        <div class="photo-left-links__wrap-img">
            
            <div class="container" data-sfcontainer container-context="@this.Model.Context.ContainerContext("Picture", "Picture")">

            </div>
            <div class="photo-left-links__btn-desktop container" data-sfcontainer container-context="@this.Model.Context.ContainerContext("Button", "Button")">
                
            </div>
            
        </div>
        
        <div class="photo-left-links__info">
            <div class="container" data-sfcontainer container-context="@this.Model.Context.ContainerContext("Title", "Title")">
                @* <h2 class="photo-left-links__title-mob title title--black title--ml">Who we are?</h2> *@
            </div>
            <div class="container" data-sfcontainer container-context="@this.Model.Context.ContainerContext("Text", "Text")">
               
            </div>
            @* <a class="photo-left-links__btn-mobile button button--primary" href="#">Read more about us</a> *@
            <div class="container" data-sfcontainer container-context="@this.Model.Context.ContainerContext("List", "List")">
                
            </div>
        </div>
    </div>
</section>
